<h1>New quantile</h1>

<% form_for(@quantile) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :bin %><br />
    <%= f.text_field :bin %>
  </p>
  <p>
    <%= f.label :characteristic_quantification_id %><br />
    <%= f.text_field :characteristic_quantification_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', quantiles_path %>