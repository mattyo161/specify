<h1>Editing characteristic_quantification</h1>

<% form_for(@characteristic_quantification) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :type %><br />
    <%= f.text_field :type %>
  </p>
  <p>
    <%= f.label :allowed_term_id %><br />
    <%= f.text_field :allowed_term_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @characteristic_quantification %> |
<%= link_to 'Back', characteristic_quantifications_path %>