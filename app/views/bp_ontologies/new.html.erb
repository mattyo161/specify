<h1>New bp_ontology</h1>

<% form_for(@bp_ontology) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :abbreviation %><br />
    <%= f.text_field :abbreviation %>
  </p>
  <p>
    <%= f.label :versionid %><br />
    <%= f.text_field :versionid %>
  </p>
  <p>
    <%= f.label :ontologyid %><br />
    <%= f.text_field :ontologyid %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', bp_ontologies_path %>